{
  "name": "request-multipart",
  "version": "0.0.1",
  "description": "Multipart Body Stream",
  "keywords": [
    "multipart",
    "form-data"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/simov/request-multipart",
  "author": "Simeon Velichkov <simeonvelichkov@gmail.com> (https://simov.github.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/simov/request-multipart.git"
  },
  "dependencies": {
    "bulk-stream": "^0.1.3",
    "isstream": "^0.1.2",
    "mime-types": "^2.1.18",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "formidable": "^1.2.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.1.1",
    "request-compose": "0.0.15",
    "request-logs": "0.0.5"
  },
  "bin": {},
  "main": "./build/index.js",
  "files": [
    "build/lib/",
    "build/index.js",
    "build/mw.js",
    "lib/",
    "index.js",
    "mw.js",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "npm run test:ci",
    "test:ci": "v=$(node -e \"console.log(process.version.split('.')[0].slice(1))\") && if [ $v -ge 8 ]; then p=\"test\"; else p=\"build/test\"; fi && mocha $p --recursive",
    "test:cov": "istanbul cover _mocha -- --recursive",
    "build:test": "babel test --out-dir build/test",
    "copy:test/fixtures": "cp -r test/fixtures/ build/test/",
    "build:lib": "babel lib --out-dir build/lib",
    "build:main": "babel *.js --out-dir build",
    "build": "rm -rf build/ && mkdir -p build && npm run build:test && npm run copy:test/fixtures && npm run build:lib && npm run build:main",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
